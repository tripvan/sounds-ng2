{
  "name": "sounds-ng2",
  "version": "1.0.0",
  "description": "Sounds Ng2 - a music search engine powered by spotify",
  "repository": {
    "type": "git",
    "url": "https://github.com/tripvan/sounds-ng2.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3000",
    "build": "ng build --prod --configuration=prod",
    "build:dev": "ng build --configuration=dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "webpack --config webpack.prod.js",
    "dev.serve": "webpack-dev-server --content-base build",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/cdk": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "bootstrap": "^3.3.6",
    "core-js": "^2.5.5",
    "rxjs": "6.1.0",
    "web-animations-js": "2.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "0.6.2",
    "codelyzer": "4.3.0",
    "cypress": "2.1.0",
    "electron": "^1.8.6",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "ts-node": "~2.0.0",
    "tslint": "5.9.1",
    "typedoc": "^0.4.3",
    "typescript": "2.7.2",
    "typings": "~1.3.0",
    "wallaby-webpack": "3.9.5",
    "@angular-devkit/build-angular": "~0.6.0"
  }
}
