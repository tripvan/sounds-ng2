{
  "name": "sounds-ng2",
  "version": "1.0.0",
  "description": "Sounds Ng2 - a music search engine powered by spotify",
  "repository": {
    "type": "git",
    "url": "https://github.com/tripvan/sounds-ng2.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 3000 --env=local",
    "build": "ng build --prod --env=prod",
    "build:dev": "ng build --env=dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "webpack --config webpack.prod.js",
    "dev.serve": "webpack-dev-server --content-base build",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "4.1.3",
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/router": "4.1.3",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "ngx-perfect-scrollbar": "4.0.0",
    "rxjs": "5.2.0",
    "web-animations-js": "2.2.2",
    "zone.js": "0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "4.1.3",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "3.0.1",
    "electron": "^1.6.11",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "ts-node": "~2.0.0",
    "tslint": "5.2.0",
    "typedoc": "^0.4.3",
    "typescript": "2.3.2",
    "typings": "~1.3.0",
    "wallaby-webpack": "0.0.38"
  }
}
